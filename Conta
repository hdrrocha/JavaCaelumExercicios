import java.util.*;
import java.util.Arrays;
import java.util.List;


public class Conta{

	int numero;
	String dono;
	double saldo;
	double limite;
	
	Conta(){
		System.out.println("Objeto conta construido!");
	}
	
	void setNumero(int numero){
		this.numero = numero;
	}
		
	int getNumero(){
		return this.numero;
	}	
	
	void setDono(String dono){
		this.dono = dono;
	}
		
	String getDono(){
		return this.dono;
	}	
	
	void setSaldo(double saldo){
		this.saldo = saldo;
	}
		
	double getSaldo(){
		return this.saldo;
	}	
	
	void setLimite(double limite){
		this.limite = limite;
	}
		
	double getLimite(){
		return this.limite;
	}
}
